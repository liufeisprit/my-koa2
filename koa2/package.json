  "main": "index.js",
  "scripts": {
    "start": "rimraf parcelDist && rimraf .cache && cross-env  NODE_ENV=development nodemon --exec \"babel-node server/index.js\"",
    "dev": "nodemon -w src --exec \"babel-node src --presets env\"",
    "production": "node dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "parcelDev": "parcel index.js",
    "parcelBuild": "rimraf parcel/parcelDist && parcel build src/index.html --no-cache -d parcelDist --public-url ./",
    "build": "rimraf dist && babel src -s -D -d dist --presets env",
    "server": "nodemon -w server --exec \"babel-node server --src --presets env\"",
    "mongod": "mongod --dbpath F:/mongoDB/data",
    "mongod2": "mongod --dbpath E:/mongdb/data"
  },
